{
  "name": "lazy-appwrite",
  "version": "0.3.0-alpha.0",
  "description": "NPM package for lazy setup with node-appwrite",
  "license": "MIT",
  "author": "ghost",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ghunter254/lazy-appwrite"
  },
  "bugs": {
    "url": "https://github.com/Ghunter254/lazy-appwrite/issues"
  },
  "homepage": "https://github.com/Ghunter254/lazy-appwrite#readme",
  "keywords": [
    "appwrite",
    "database",
    "schema",
    "lazy",
    "sync",
    "node-appwrite",
    "orm",
    "odm"
  ],
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "dependencies": {
    "node-appwrite": "^20.3.0"
  }
}
